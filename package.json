{
  "name": "inventory",
  "version": "4.0.0",
  "description": "Little helper application to manage (cellar) inventory.",
  "author": "Raphael Horber (raphael.horber@gmail.com)",
  "private": true,
  "scripts": {
    "dev": "nuxt dev --dotenv .env.development",
    "devv": "yarn dev -o",
